// an object can borrow a method from another object.

const person = {
	firstName : "Jhon",
	lastName : "Doe",
	fullName : function(){
		return this.firstName + " " + lastName;
	}
}

const member = {
	firstName : "Elon",
	lastName : "Musk",
}


let fullName  = person.fullName.bind(member);
console.log(fullName)

